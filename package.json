{
    "name": "@kant2002-diia-inhouse/gateway-service",
    "version": "1.75.1",
    "main": "dist/index.js",
    "repository": "https://github.com/diia-open-source/be-gateway-service.git",
    "author": "Diia",
    "license": "SEE LICENSE IN LICENSE.md",
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "prepare": "ts-patch install -s",
        "build": "rimraf dist/ && tsc",
        "start": "node dist/index.js",
        "dev": "tsc-watch --skipLibCheck --onSuccess 'node dist/index.js' | pino-pretty -S",
        "semantic-release": "semantic-release",
        "lint": "eslint --ext .ts . && prettier --check .",
        "lint-fix": "eslint '*/**/*.{js,ts}' --fix && prettier --write .",
        "lint:lockfile": "lockfile-lint --path package-lock.json --allowed-hosts registry.npmjs.org --validate-https",
        "test": "jest",
        "test:integration": "npm run test -- --selectProjects integration",
        "test:unit": "npm run test -- --runInBand --selectProjects unit",
        "test:coverage": "npm run test -- --selectProjects unit --coverage",
        "format": "prettier --write .",
        "migrate-deploy": "npm run migrate up",
        "migrate-test": "NODE_ENV=test npm run migrate up",
        "migrate": "sh -c 'ts-node --project migrations/tsconfig.json node_modules/.bin/migrate-mongo $0 $1 -f migrate-mongo-config.ts'",
        "indexes:sync": "MONGO_INDEXES_SYNC=true MONGO_INDEXES_EXIT_AFTER_SYNC=true npm run start",
        "find-circulars": "madge --circular --extensions ts ./",
        "scaffold": "scaffold"
    },
    "dependencies": {
        "@kant2002-diia-inhouse/cms": "^1.4.0",
        "@kant2002-diia-inhouse/crypto": "^1.7.0",
        "@kant2002-diia-inhouse/db": "^3.1.1",
        "@kant2002-diia-inhouse/diia-app": "^12.2.0",
        "@kant2002-diia-inhouse/diia-logger": "^2.10.0",
        "@kant2002-diia-inhouse/diia-metrics": "^3.6.0",
        "@kant2002-diia-inhouse/diia-queue": "^5.10.1",
        "@kant2002-diia-inhouse/env": "^1.5.0",
        "@kant2002-diia-inhouse/errors": "^1.6.1",
        "@kant2002-diia-inhouse/healthcheck": "^1.6.0",
        "@kant2002-diia-inhouse/http": "^1.5.3",
        "@kant2002-diia-inhouse/redis": "^2.4.0",
        "@kant2002-diia-inhouse/types": "^5.1.0",
        "@kant2002-diia-inhouse/utils": "^2.36.1",
        "@kant2002-diia-inhouse/validators": "^1.13.1",
        "axios": "1.6.7",
        "compare-versions": "6.1.0",
        "ejs": "3.1.9",
        "formidable": "2.1.2",
        "lodash": "4.17.21",
        "minio": "7.1.1",
        "module-alias": "2.2.3",
        "mongoose": "8.2.0",
        "multer": "1.4.5-lts.1",
        "p-timeout": "4.1.0",
        "pluralize": "8.0.0",
        "uuid": "9.0.1"
    },
    "devDependencies": {
        "@kant2002-diia-inhouse/configs": "^1.27.1",
        "@kant2002-diia-inhouse/eslint-config": "^3.5.0",
        "@kant2002-diia-inhouse/scaffold": "^2.8.0",
        "@kant2002-diia-inhouse/test": "^5.1.2",
        "@types/ejs": "3.1.3",
        "@types/express": "4.17.18",
        "@types/formidable": "2.0.5",
        "@types/lodash": "4.14.199",
        "@types/multer": "1.4.11",
        "@types/node": "20.7.1",
        "@types/pluralize": "0.0.31",
        "@types/uuid": "9.0.4",
        "jest-mock-extended": "3.0.5",
        "lockfile-lint": "4.12.1",
        "madge": "6.1.0",
        "openapi-types": "12.1.3",
        "rimraf": "5.0.5",
        "type-fest": "4.12.0"
    },
    "_moduleAliases": {
        "@services": "dist/services",
        "@interfaces": "dist/interfaces",
        "@models": "dist/models",
        "@dataMappers": "dist/dataMappers",
        "@utils": "dist/utils",
        "@xmlMappings": "dist/xmlMappings",
        "@actions": "dist/actions",
        "@src": "dist",
        "@mocks": "tests/mocks",
        "@validation": "dist/validation"
    },
    "overrides": {
        "bson": "6.3.0"
    },
    "release": {
        "extends": "@kant2002-diia-inhouse/configs/dist/semantic-release/service"
    },
    "commitlint": {
        "extends": "@kant2002-diia-inhouse/configs/dist/commitlint"
    },
    "eslintConfig": {
        "extends": "@kant2002-diia-inhouse/eslint-config"
    },
    "prettier": "@kant2002-diia-inhouse/eslint-config/prettier",
    "madge": {
        "tsConfig": "./tsconfig.json"
    },
    "jest": {
        "preset": "@kant2002-diia-inhouse/configs/dist/jest"
    }
}
